<resources xmlns:tools="http://schemas.android.com/tools">

    <!-- Base application theme. -->
    <style name="QamaiTheme" parent="Theme.AppCompat.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="android:windowBackground">@color/QamaiThemeColor</item>
        <item name="android:colorBackground">@color/QamaiThemeColor</item>
        <item name="android:windowAnimationStyle">@null</item>
        <item name="android:windowLightStatusBar" tools:targetApi="23">true</item>
        <item name="android:windowLightStatusBar" tools:targetApi="23" tools:ignore="DuplicateDefinition">true</item>
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>


</resources>
